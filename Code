Private Sub Form_Load()
Label38.Caption = Form2.Text3.Text & " " & Form2.Text4.Text
Label39.Caption = Form2.Text5.Text
Label41.Caption = Form2.Text7.Text & " " & Form2.Text10.Text
Text1.Text = Form3.Text22.Text
Text2.Text = Form3.Text26.Text
Text3.Text = Form3.Text30.Text
Text4.Text = Form3.Text34.Text
Text5.Text = Form3.Text38.Text
Text6.Text = Form3.Text42.Text
Text7.Text = Val(Text1.Text) + Val(Text2.Text) + Val(Text3.Text) + Val(Text4.Text) + Val(Text5.Text) + Val(Text6.Text)
Text8.Text = "18%"
Dim a As Integer
a = (Val(Text7.Text) * 18) / 100
Text9.Text = Val(Text7.Text) + a
Label9.Caption = Form3.Text43.Text
Label12.Caption = Form3.Text44.Text
Label15.Caption = Form3.Text45.Text
Label18.Caption = Form3.Text46.Text
Label21.Caption = Form3.Text47.Text
Label24.Caption = Form3.Text48.Text
Label49.Caption = Label9.Caption
Label50.Caption = Label12.Caption
Label55.Caption = Label15.Caption
Label57.Caption = Label18.Caption
Label59.Caption = Label21.Caption
