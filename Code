Private Sub Timer1_Timer()
ProgressBar1.Value = ProgressBar1.Value + 5
Label65.Caption = ProgressBar1.Value & "%"
   If (ProgressBar1.Value = ProgressBar1.Max) Then
   Timer1.Enabled = False
   Frame2.Visible = True
   End If
End Sub
