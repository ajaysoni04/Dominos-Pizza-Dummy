Private Sub Command2_Click()
If Text1.Text = "" Or Text2.Text = "" Then
MsgBox "mobile no or password cannot be blank"
End If
If Text2.Text = Text11.Text Then
 If Text1.Text = Val(Text5.Text) Then
 Form2.Visible = False
 Form3.Visible = True
 End If
Else
MsgBox "wrong password !!!!"
End If
If Text1.Text = Text5.Text Then
Else
MsgBox "wrong mobile no entered!!!"
End If
 End Sub

